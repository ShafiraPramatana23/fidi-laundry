<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:id="@+id/llItem"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_marginTop="@dimen/_12sdp"    android:orientation="horizontal">    <ImageView        android:id="@+id/ivIcon"        android:layout_width="@dimen/_38sdp"        android:layout_height="@dimen/_38sdp"        android:scaleType="centerCrop"        android:src="@drawable/rounded_light_small" />    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginLeft="@dimen/_12sdp"        android:orientation="vertical">        <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="vertical">                <com.fidilaundry.app.util.FontTextView                    android:id="@+id/tvTitle"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerVertical="true"                    android:text="Cuci Setrika"                    android:textColor="@color/black"                    android:textSize="@dimen/_12sdp"                    app:fontName="@string/font_name_dmsans"                    app:textStyle="@integer/font_style_semibold" />                <com.fidilaundry.app.util.FontTextView                    android:id="@+id/tvDesc"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginTop="@dimen/_5sdp"                    android:layout_centerVertical="true"                    android:text="15 Jun 2022 11:11"                    android:textColor="@color/gray_dark"                    android:textSize="@dimen/_10sdp"                    app:fontName="@string/font_name_dmsans"                    app:textStyle="@integer/font_style_reguler" />                <com.fidilaundry.app.util.FontTextView                    android:id="@+id/tvStatus"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginBottom="@dimen/_10sdp"                    android:layout_centerVertical="true"                    android:text="Status : Setrika"                    android:textColor="@color/gray_dark"                    android:textSize="@dimen/_10sdp"                    app:fontName="@string/font_name_dmsans"                    app:textStyle="@integer/font_style_reguler" />            </LinearLayout>            <com.fidilaundry.app.util.FontTextView                android:id="@+id/tvPrice"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:text=""                android:textColor="@color/blackHistory"                android:textSize="@dimen/_12sdp"                app:fontName="@string/font_name_dmsans"                app:textStyle="@integer/font_style_reguler" />        </RelativeLayout>        <!--        <View-->        <!--            android:id="@+id/line"-->        <!--            android:layout_width="match_parent"-->        <!--            android:layout_height="0.5dp"-->        <!--            android:layout_marginTop="@dimen/_6sdp"-->        <!--            android:background="@color/gray_dark"-->        <!--            android:visibility="gone" />-->    </LinearLayout></LinearLayout>